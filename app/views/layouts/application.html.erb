<!DOCTYPE html>
<html>
  <head>
      <title>Keeda</title>
      <meta charset="utf-8">
      <%= csrf_meta_tags %>
      <%= csp_meta_tag %>
      <%= action_cable_meta_tag %>
      <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
      <%= javascript_include_tag Ckeditor.cdn_url, 'data-turbolinks-track': 'reload' %>
    <script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['GOOGLE_MAPS_API_KEY'] %>&libraries=places&callback=initMap" defer data-turbolinks-eval="false"></script>

  </head>
  <body class="sidebar-mini">
      <%= render partial: "layouts/header" %>
      <div class="container-fluid">
          <p class="notice"><%= notice %></p>
          <p class="alert"><%= alert %></p>
          <%= yield %>
          <%= debug(params) if Rails.env.development? %>
          <%= render partial: "layouts/footer" %>
      </div>
  </body>
</html>
